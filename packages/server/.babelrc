{
	"presets": ["@babel/preset-env"],
    ["es2015", { "modules": false}],
    "es2016",
    "stage-2",
    "stage-0",
    ],
  "env": {
    "server": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
	},
	"plugins": [
		[
			"@babel/plugin-transform-runtime"
		],
		[
			"@babel/plugin-proposal-class-properties"
		]
	]
}
