{
	"presets": [
		["@babel/preset-env"],
    ["@babel/preset-es2015", { "modules": false}],
    ["@babel/preset-es2016"],
    ["@babel/preset-stage-2"],
    ["@babel/preset-stage-0"]
	],
  "env": {
    "server": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
	},
	"plugins": [
		[
			"@babel/plugin-transform-runtime"
		],
		[
			"@babel/plugin-proposal-class-properties"
		]
	]
}
