argin=\"0 0 20px 0\">\n          <Box justifycontent=\"flex-start\" flexdirection=\"row\">\n            <Text>Sort By: </Text>\n            <select>\n              <option>Room</option>\n              <option>Self</option>\n            </select>\n          </Box>\n          <Box flexdirection=\"row\" justifycontent=\"flex-start\">\n            {' '}<Text>Sort By: </Text>\n            <select>\n              <option>words</option>\n              <option>uttered</option>\n            </select>\n            <select>\n              <option>Asc</option>\n              <option>Desc</option>\n            </select>\n          </Box>\n        </Box>\n        <Box alignitems=\"flex-start\">\n          {' '}<UtteredList utteredList={this.props.reducer.utteredList} />\n        </Box>\n      </Box>\n    )\n  }\n}\n\nconst mapStateToProps = state => {\n  return {\n    reducer: state.utteredReducer\n  }\n}\n\nconst mapDispatchToProps = dispatch => {\n  return {\n    action: bindActionCreators({loadUtteredList, updateUtteredList}, dispatch)\n  }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(UtteredContainer)\n"},"lineCount":null}},"hash":"631619de704b2dfd552527a0e5372d8d","cacheData":{"env":{}}}