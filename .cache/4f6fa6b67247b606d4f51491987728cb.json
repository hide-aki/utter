Admin/Courses/containers/Courses.js","original":{"line":63,"column":40}},{"generated":{"line":141,"column":64},"source":"layouts/Admin/Courses/containers/Courses.js","original":{"line":63,"column":15}},{"name":"mapStateToProps","generated":{"line":155,"column":26},"source":"layouts/Admin/Courses/containers/Courses.js","original":{"line":11,"column":6}},{"name":"Courses","generated":{"line":156,"column":26},"source":"layouts/Admin/Courses/containers/Courses.js","original":{"line":34,"column":6}}],"sources":{"layouts/Admin/Courses/containers/Courses.js":"import React, {Component} from 'react'\nimport {connect} from 'react-redux'\n\nimport {Grid, Segment, Header} from 'semantic-ui-react'\n\nimport orm from '../../../../app/schema.js'\n\nimport CoursesList from '../components/CoursesList'\nimport CourseDetails from '../components/CourseDetails'\n\nconst mapStateToProps = state => {\n  const session = orm.session(state.entities)\n  const {Courses} = session\n  let courses = Courses.all()\n    .toModelArray()\n    .map(courseModel => {\n      const courses = {\n        // Copy the data from the plain JS object\n        ...courseModel.ref,\n        // Provide a default empty object for the relation\n        courseType: {}\n      }\n\n      if (courseModel.type) {\n        // Replace the default object with a copy of the relation's data\n        courses.courseType = {...courseModel.type.ref}\n      }\n      return courses\n    })\n  courses = [{name: 'hi'}]\n  return {courses}\n}\n\nclass Courses extends Component {\n  componentDidMount() {\n    // load courses from db\n  }\n\n  render() {\n    const {courses = []} = this.props\n\n    const currentCourse = courses[0] || {}\n\n    return (\n      <Segment>\n        <Grid>\n          <Grid.Column width={10}>\n            <Header as=\"h3\">Courses List</Header>\n            <CoursesList courses={courses} />\n          </Grid.Column>\n          <Grid.Column width={6}>\n            <Header as=\"h3\">Course Details</Header>\n            <Segment>\n              <CourseDetails course={currentCourse} />\n            </Segment>\n          </Grid.Column>\n        </Grid>\n      </Segment>\n    )\n  }\n}\n\nexport default connect(mapStateToProps)(Courses)\n"},"lineCount":null}},"hash":"1177317c785be9d43a9c84847332dbb7","cacheData":{"env":{}}}