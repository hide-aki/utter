ToProps = dispatch => {\n  return {\n    actions: bindActionCreators(\n      {\n        toggleFooter\n      },\n      dispatch\n    )\n  }\n}\n\nexport default connect(\n  null,\n  mapDispatchToProps\n)(Settings)\n"},"lineCount":null}},"hash":"7ea9af0031aef1a8d359733be7c305b9","cacheData":{"env":{}}}